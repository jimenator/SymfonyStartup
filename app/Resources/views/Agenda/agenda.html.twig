{% extends 'base.html.twig' %}

{% block body %}
    <table id="telephoneTable">
            <tr>
                <th>{{ 'Agenda.name' | trans | capitalize }}</th>
                <th>{{ 'Agenda.telephone' | trans | capitalize }}</th>
            </tr>
        {% for numberRow in telefonNumbers %}
            <tr>
                <td>{{ numberRow['name'] }}</td>
                <td>{{ numberRow['telefonNumber'] }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}